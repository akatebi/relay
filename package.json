  "scripts": {
    "clean": "find src/component -name __generated__ -type d -exec rm -rf {} +",
    "touch": "find src/component -name \\*.js -exec touch -c {} +",
    "xtouch": "touch ./src/**/*.js ./src/**/**/*.js ./src/**/**/**/*.js ./src/**/**/**/**/*.js",
    "unix2dos": "unix2dos -q ./src/**/*.js ./src/**/**/*.js ./src/**/**/**/*.js ./src/**/**/**/**/*.js",
    "prerelay": "yarn run update-schema && yarn run clean && yarn run touch && sleep 10",
    "relay": "relay-compiler --src ./src/component --schema server/schema.graphql",
    "prestart": "yarn update-schema",
    "start": "pm2 start development.yml --env development --no-daemon",
    "update-schema": "node -r babel-register server/updateSchema.js",
    "prebuild": "rm -rf dist && yarn run relay",
    "build": "pm2 start production.yml --env production",
    "postbuild": "webpack --color --define NODE_ENV=production",
    "graphql": "pm2 start development.yml --only graphql --no-daemon",
    "schema": "pm2 start development.yml --only schema --no-daemon",
    "web": "pm2 start development.yml --only web",
    "docker:build": "docker build -t akatebi/graphql .",
    "docker:run": "docker run -v /media/$USER:/media/$USER -p 8086:8086 -p 3006:3006 -d --name graphql --restart unless-stopped akatebi/graphql",
    "docker:sh": "docker exec -it graphql /bin/sh",
    "docker:stop": "docker stop graphql && docker rm graphql",
    "test": "jest --config jest/config.json",
    "test:query": "jest --config jest/config.json --testPathPattern=base",
    "webpack-dev-server": "webpack-dev-server --inline --content-base . --history-api-fallback",
    "profile": "webpack --profile --json > stats.json",
    "analyze": "firefox http://webpack.github.io/analyse",
    "tar": "tar cvzf ~/relay.tgz ../relay --exclude-from=.gitignore"
  },
